{
  "name": "gibuvaroch",
  "version": "0.1.0",
  "description": "Encrypted search",
  "repository": {
    "type": "git",
    "url": "https://github.com/nitzantomer/gibuvaroch"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Kin+Orbs",
  "license": "MIT",
  "scripts": {
    "build": "yarn run build-ts && yarn run tslint",
    "build-ts": "tsc && tsc -p webclient/scripts/tsconfig.json",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "tsc -p tsconfig.test.json && NODE_ENV=test ./node_modules/.bin/mocha distTest/test/flow.spec.js --exit ",
    "contract-test": "tsc -p tsconfig.test.json && NODE_ENV=test ./node_modules/.bin/truffle test ./test/contract.spec.js",
    "start-node": "./node_modules/.bin/ganache-cli -p 8545 -l 300000000000000 -e 500000",
    "contract-compile": "./node_modules/.bin/truffle compile"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "lodash": "4.17.5",
    "redis": "^2.8.0",
    "redis-commands": "^1.3.5",
    "stylus": "^0.54.5",
    "then-redis": "^2.0.1",
    "web3": "^1.0.0-beta.34"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/chai": "^4.1.2",
    "@types/express": "^4.11.1",
    "@types/lodash": "^4.14.99",
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.4.4",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/redis": "^2.8.6",
    "@types/shelljs": "^0.7.8",
    "@types/sinon-chai": "^2.7.29",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "ganache-cli": "^6.1.0",
    "ganache-core": "^2.1.0",
    "mocha": "^5.1.1",
    "sinon-chai": "^3.0.0",
    "truffle": "^4.1.7",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
